!!! 5
%html{ lang: "en" }
  %head
    %title Open Source &amp; Feelings
    -# = stylesheet_link_tag "normalize", "theme", "https://fonts.googleapis.com/css?family=Lato:300,600,900"
    = stylesheet_link_tag "all"
    %link{ rel: "icon", href: "/images/favicon.ico", type: "image/x-icon" }

    %meta{ content: "width=device-width, initial-scale=1.0", name: "viewport" }

  %body
    = partial :header

    %main.container
      = yield

    = partial :footer

    -# Placed at the end of the document so the pages load faster
    = javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js", "all"
